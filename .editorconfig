# Normalizes editor configuration.
# http://editorconfig.org
root = true

[*]
indent_style = tab
indent_size = tab
end_of_line = LF
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.js]
indent_style = tab
indent_size = tab

[*.css]
indent_style = tab
indent_size = tab

[*.ejs]
indent_style = tab
indent_size = tab

[*.md]
trim_trailing_whitespace = false
